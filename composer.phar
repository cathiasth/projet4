}


 $ah = fopen($a, 'rb');
$bh = fopen($b, 'rb');

$result = true;
while (!feof($ah)) {
if (fread($ah, 8192) != fread($bh, 8192)) {
$result = false;
break;
}
}

fclose($ah);
fclose($bh);

return $result;
}
